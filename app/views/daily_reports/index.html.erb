<p id="notice"><%= notice %></p>

<h1>Daily Reports</h1>

<table>
  <thead>
    <tr>
      <th>Report date</th>
      <th>Age</th>
      <th>Remedy</th>
      <th>Feed recieved</th>
      <th>Feed actual</th>
      <th>Feed remains</th>
      <th>Average weight</th>
      <th>Mortality</th>
      <th>Average temperature</th>
      <th>Average hummidity</th>
      <th>Stockman</th>
      <th>Notes</th>
      <th>Periode</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @daily_reports.each do |daily_report| %>
      <tr>
        <td><%= daily_report.report_date %></td>
        <td><%= daily_report.age %></td>
        <td><%= daily_report.remedy %></td>
        <td><%= daily_report.feed_recieved %></td>
        <td><%= daily_report.feed_actual %></td>
        <td><%= daily_report.feed_remains %></td>
        <td><%= daily_report.average_weight %></td>
        <td><%= daily_report.mortality %></td>
        <td><%= daily_report.average_temperature %></td>
        <td><%= daily_report.average_hummidity %></td>
        <td><%= daily_report.stockman %></td>
        <td><%= daily_report.notes %></td>
        <td><%= daily_report.periode %></td>
        <td><%= link_to 'Show', daily_report %></td>
        <td><%= link_to 'Edit', edit_daily_report_path(daily_report) %></td>
        <td><%= link_to 'Destroy', daily_report, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Daily Report', new_daily_report_path %>
