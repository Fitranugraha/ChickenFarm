<p id="notice"><%= notice %></p>

<h1>Tabel Standar Acuan</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Umur (Hari)</th>
      <th>Berat Rata - Rata</th>
      <th>Kematian (%)</th>
      <th>FCR</th>
      <th>Std EEF</th>
	  <!--
      <th colspan="3"></th>
	  -->
    </tr>
  </thead>

  <tbody>
    <% @standards.each do |standard| %>
      <tr>
        <th scope="row"><%= standard.age_day %></td>
        <td><%= standard.avg_bw %></td>
        <td><%= standard.percent_mort %></td>
        <td><%= standard.fcr %></td>
        <td><%= standard.std_eef %></td>
		<!--
        <td><%= link_to 'Show', standard %></td>
        <td><%= link_to 'Edit', edit_standard_path(standard) %></td>
        <td><%= link_to 'Destroy', standard, method: :delete, data: { confirm: 'Are you sure?' } %></td>
		-->
      </tr>
    <% end %>
  </tbody>
</table>

<br>

